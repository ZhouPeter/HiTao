import UIKit



class MineStoryboardHelper: NSObject {
    
    static let storyboardName = "Mine"
    static let navigationController = "mineNavigationController"
    class func instantiateViewController( identifier: String) -> UIViewController {
        let storyboard:UIStoryboard!
        storyboard = UIStoryboard(name:self.storyboardName, bundle: nil)
        let viewController = storyboard.instantiateViewController(withIdentifier: identifier)
        return viewController
    }
    
}

